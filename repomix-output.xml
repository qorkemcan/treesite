// astro.config.mjs
import { defineConfig } from 'astro/config';
import sitemap from '@astrojs/sitemap';
import vercel from '@astrojs/vercel';

export default defineConfig({
  site: 'https://www.protreetrim.com',
  output: 'static', 
  integrations: [
    sitemap({
      // Her sayfa i√ßin √∂zel SEO kurallarƒ± tanƒ±mlƒ±yoruz
      serialize(item) {
        // ≈ûehir bazlƒ± programmatic sayfalar (tree-removal, stump-grinding, emergency-service)
        if (
          item.url.includes('tree-removal') || 
          item.url.includes('stump-grinding') || 
          item.url.includes('emergency-service')
        ) {
          return {
            ...item,
            changefreq: 'weekly', // Fiyatlar veya veriler deƒüi≈ütik√ße haftalƒ±k taransƒ±n
            priority: 0.9,       // Bu sayfalar bizim en √∂nemli sayfalarƒ±mƒ±z (Hero pages)
            lastmod: new Date().toISOString(), // Her build alƒ±ndƒ±ƒüƒ±nda g√ºncel tarih basar
          };
        }
        
        // Ana sayfa i√ßin en y√ºksek √∂ncelik
        if (item.url === 'https://www.protreetrim.com/') {
          return {
            ...item,
            changefreq: 'daily',
            priority: 1.0,
          };
        }

        // Diƒüer statik sayfalar (About, Privacy, etc.)
        return {
          ...item,
          changefreq: 'monthly',
          priority: 0.5,
        };
      },
    }),
  ],
  adapter: vercel(),
});
